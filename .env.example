# Shopmonkey API Configuration
SHOPMONKEY_API_TOKEN=your-shopmonkey-api-token-here
SHOPMONKEY_API_BASE_URL=https://api.shopmonkey.cloud

# Google Sheets Configuration
GOOGLE_SHEETS_ID=your-google-sheets-id-here
GOOGLE_APPLICATION_CREDENTIALS=./credential.json

# Optional: Location ID for Shopmonkey (if you have multiple locations)
SHOPMONKEY_LOCATION_ID=

# Optional: API key for authentication (if not set, endpoints are public)
API_KEY=

# Optional: CORS allowed origins (comma-separated, e.g., "https://example.com,https://app.example.com")
# If not set, CORS is disabled. Set to "*" only for development.
ALLOWED_ORIGINS=
